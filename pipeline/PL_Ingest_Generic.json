{
	"name": "PL_Ingest_Generic",
	"properties": {
		"activities": [
			{
				"name": "Lookup - Get Sources",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OdbcSource",
						"query": {
							"value": "@concat(\n  'SELECT * FROM dbo.md_Source WHERE Active = 1',\n  if(\n    equals(pipeline().parameters.pSourceName, ''),\n    '',\n    concat(' AND SourceName = ''', pipeline().parameters.pSourceName, '''')\n  )\n)\n",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00"
					},
					"dataset": {
						"referenceName": "DS_SQL",
						"type": "DatasetReference",
						"parameters": {
							"pTable": "dbo.md_Source",
							"pWhere": "''",
							"pQueryMode": "Query"
						}
					},
					"firstRowOnly": false
				}
			}
		],
		"parameters": {
			"pSourceName": {
				"type": "string"
			},
			"pRunDate": {
				"type": "string",
				"defaultValue": "@{utcNow()}"
			},
			"pActiveOnly": {
				"type": "string",
				"defaultValue": "1"
			}
		},
		"variables": {
			"vWaterMark": {
				"type": "String"
			}
		},
		"annotations": []
	}
}